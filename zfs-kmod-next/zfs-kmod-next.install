post_install() {
    EXTRAMODULES='extramodules-6.7-next'
    depmod -a $(cat /lib/modules/$EXTRAMODULES/version)
}

post_upgrade() {
    EXTRAMODULES='extramodules-6.7-next'
    depmod -a $(cat /lib/modules/$EXTRAMODULES/version)
}

post_remove() {
    EXTRAMODULES='extramodules-6.7-next'
    depmod -a $(cat /lib/modules/$EXTRAMODULES/version)
    
}
